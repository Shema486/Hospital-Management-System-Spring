type Department{
    deptID:ID!
    locationFloor:Int
    deptName:String!
}

input DepartmentRequest{
    locationFloor:Int!
    deptName:String!
}
extend type Query{
    departments:[Department!]!
    departmentById(id:ID!):Department

}
extend type Mutation{
    addDepartment(input:DepartmentRequest!):Department
    deleteDepartment(id:ID!):String!
    updateDepartment(id:ID!, input:DepartmentRequest):Department
}
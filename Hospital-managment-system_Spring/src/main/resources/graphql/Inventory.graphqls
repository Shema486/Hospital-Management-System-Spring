type Inventory{
    itemId:ID!
    itemName:String!
    StockQuantity:Int
    unitPrice:Float
}

input InventoryRequest{
    itemName:String!
    StockQuantity:Int!
    unitPrice:Float!
}

extend type Query{
    inventories:[Inventory!]!
}
extend type Mutation{
    addInventory(input:InventoryRequest!):Inventory
    deleteInventory(id:ID):String!
    updateInventory(idd:ID, input:InventoryRequest):Inventory
}
